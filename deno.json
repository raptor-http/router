{
  "name": "@raptor/router",
  "version": "0.11.0",
  "exports": {
    ".": "./mod.ts",
    "./types": "./src/types.ts"
  },
  "publish": {
    "exclude": [
      ".github",
      "examples",
      "docs",
      "**/*.test.ts"
    ]
  },
  "tasks": {
    "test": "APP_URL=http://localhost:8000 deno test --coverage=coverage-report --allow-all --parallel",
    "test-coverage": "APP_URL=http://localhost:8000 deno coverage coverage-report"
  },
  "fmt": {
    "exclude": [
      "README.md"
    ]
  },
  "lock": true,
  "imports": {
    "@raptor/framework": "jsr:@raptor/framework@*",
    "@std/assert": "jsr:@std/assert@^1.0.16"
  },
  "peerDependencies": {
    "@raptor/framework": "*"
  }
}
